# Configuração de Ambiente - YOLO Benchmark System
# Copie este arquivo para .env e ajuste os valores

# Caminhos do projeto
PROJECT_ROOT=/home/mingas/projetos/trabalhoPDI
DATA_PATH=/home/mingas/projetos/trabalhoPDI/src/data
MODELS_PATH=/home/mingas/projetos/trabalhoPDI/src/models
RESULTS_PATH=/home/mingas/projetos/trabalhoPDI/src/results

# Configurações de treinamento
DEFAULT_MODEL=yolo11n.pt
DEVICE=cuda
WORKERS=8
BATCH_SIZE=16
EPOCHS=100
IMAGE_SIZE=640

# Dataset
DATASET_PATH=/path/to/your/data.yaml

# Otimização
OPTIMIZER=Adam
LEARNING_RATE=0.01
MOMENTUM=0.937
WEIGHT_DECAY=0.0005

# Benchmark
DEFAULT_BENCHMARK_NAME=yolo_benchmark
NUM_DIVISIONS=5
PARALLEL_EXECUTION=false
MAX_WORKERS=2

# Outros
RANDOM_SEED=42
VERBOSE=true
SAVE_CHECKPOINTS=true
